cmap <C-n> <Down>

imap <C-d> <Esc><C-d>
imap <C-u> <Esc><C-u>
map <C-s> :source ~/.vimperatorrc
map \s :source ~/.vimperatorrc

noremap j 5j
noremap k 5k
noremap h 5h
noremap l 5l
